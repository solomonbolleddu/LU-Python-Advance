    "from collections import Counter\n",
    "class MyCounter(Counter):\n",
    "    def __call__(self,Counter):\n",
    "        self.Counter = Counter\n",
    "        print(Counter)\n",
    "        \n",
    "co1 = MyCounter()\n",
    "co1(\"intel\")"
